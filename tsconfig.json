{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "outDir": "./dist"
    },
    "exclude": ["node_modules", "dist", "**/*.test.ts"]
  }
  